package trickyoutput;

class Person {
	private String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
}

public class OOP {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	Person p = new Person();
				p.setName("abc");
				doSomething(p);
				System.out.println(p.getName());
				}
	
	public void	doSomething (Person p){
				p.setname("pqr");
				p = new Person();
				p.setname("xyz");

	}

}
